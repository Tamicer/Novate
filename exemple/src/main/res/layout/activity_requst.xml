<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".ExempleActivity"
    android:orientation="vertical">

    <Button
        android:id="@+id/get_String"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="get_String"
        />

    <Button
        android:id="@+id/get_Bitmap"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="get_Bitmap"
        />

    <Button
        android:id="@+id/get_Entity"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="get_Entity"
        />

    <Button
        android:id="@+id/get_ListEntity"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="get_ListEntity"
        />



    <Button
        android:id="@+id/get_File"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Post"
        />


</LinearLayout>
